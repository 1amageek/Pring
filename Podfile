# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Pring' do
  use_frameworks!

  pod 'Firebase'
  pod 'Firebase/Firestore'
  pod 'Firebase/Storage'

  target 'PringTests' do
    inherit! :search_paths
  end
end

target 'Sample' do
  use_frameworks!
  pod 'Firebase'
  pod 'Firebase/Firestore'
  pod 'Firebase/Storage'
  pod 'Pring', :path => './'

end
